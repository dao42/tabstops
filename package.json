{
  "name": "tabstops",
  "description": "JavaScript library and API for parsing, compiling and rendering snippets with tabstops.",
  "version": "0.1.2",
  "homepage": "https://github.com/jonschlinkert/tabstops",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/tabstops",
  "bugs": {
    "url": "https://github.com/jonschlinkert/tabstops/issues"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "license": "MIT",
  "scripts": {
    "test": "mocha",
    "cover": "nyc --reporter=text --reporter=html mocha"
  },
  "keywords": [
    "tabstop",
    "tabstops"
  ],
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "enquirer": "file:../../@enquirer/enquirer-3.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "^2.0.0",
    "log-update": "^3.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
